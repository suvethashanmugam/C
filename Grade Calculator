#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int marks;

    scanf("%d", &marks);

    if (marks >= 76 && marks <= 100) {

        printf("A\n");

    }

    else if (marks >= 51 && marks <= 75) {

        printf("B\n");

    }

    else if (marks >= 26 && marks <= 50) {

        printf("C\n");

    }

    else {

      printf("D\n");
return 0;
}
