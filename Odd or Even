#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    long int x;
    scanf("%ld",&x);
    if(x<0)
        printf("Invalid Input");
    else if(x%2==1)
        printf("Odd");
    else
        printf("Even");
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
