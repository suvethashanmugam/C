#include <stdio.h>
int main() {
    unsigned long long n;
    int i, flag = 0;
    scanf("%llu", &n);
    if (n == 1) {
        printf("Neither prime nor Composite\n");
        return 0;
    }
    for (i = 2; i * (unsigned long long)i <= n; i++) {
        if (n % i == 0) {
            flag = 1;
            break;
        }
    }
    if (flag == 0)
        printf("Yes\n");  
    else
        printf("No\n");  
   
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

